{
  "name": "boilderplateReact",
  "version": "1.0.0",
  "description": "There is a boilderplate for Webpack React and MongoDB",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec babel-node ./server/server.js --ignore views/",
    "build:dev": "webpack --env=dev --progress --profile --colors --watch",
    "build:dist": "webpack --env=prod --progress --profile --colors",
    "insertdata": "node ./database/myDataMongo.js",
    "updatedata": "node ./database/myUpdateMongo.js",
    "deletedata": "node ./database/myDeleteCollection.js"
  },
  "author": "Anh(Robert)T. NGUYEN",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.17.2",
    "chunk-manifest-webpack-plugin": "^1.1.0",
    "css-loader": "^0.28.4",
    "ejs": "^2.5.7",
    "ejs-html-loader": "^3.1.0",
    "ejs-loader": "^0.3.0",
    "express": "^4.15.3",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "jquery": "^3.2.1",
    "mongodb": "^2.2.31",
    "nodemon": "^1.12.1",
    "prop-types": "^15.5.10",
    "pug": "^2.0.0-rc.3",
    "raw-loader": "^0.5.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "redux-logger": "^3.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.3.0",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-manifest-plugin": "^1.2.1",
    "webpack-merge": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "extract-text-webpack-plugin": "^3.0.0",
